  <!ELEMENT header_yr (#PCDATA) >
  <!ELEMENT header_isbn (#PCDATA) >
  <!ELEMENT header_price (#PCDATA) >
  <!ELEMENT książka (tytuł, autor, rok_wydania?, isbn?, cena?) >
  <!ATTLIST książka id ID #REQUIRED>
  <!ELEMENT tytuł (#PCDATA) >
  <!ELEMENT autor (#PCDATA) >
  <!ELEMENT rok_wydania (#PCDATA) >
  <!ELEMENT isbn (#PCDATA) >
  <!ELEMENT cena (#PCDATA) >
]>
  

